<h1>Artigos</h1>

<ul class="post-loop flex">

    <% for(article in public.articles._data) { %>
        <li class="post-item">
            <a href="/articles/<%- article %>.html" >
                <span class="post-title">
                    <%- public.articles._data[article].title %>
                </span>

                <span class="post-desc">
                    <%- public.articles._data[article].description %>
                </span>

                <span class="post-date">
                    <%- public.articles._data[article].date%>
                </span>
            </a>
        </li>
    <% } %>
</ul>

<div id="disqus_thread"></div>
<script type="text/javascript">
    /* * * CONFIGURATION VARIABLES * * */
    var disqus_shortname = 'niteroilongboardgithubio';

    /* * * DON'T EDIT BELOW THIS LINE * * */
    (function() {
        var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
        dsq.src = '//' + disqus_shortname + '.disqus.com/embed.js';
        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
    })();
</script>
